@import "../../app";
.main {
  background-color: rgba(245, 245, 245, 1);
  min-height: 100vh;
}

.swiper {
  padding: 20px;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
}

.swiperItem {
  /* padding: 20px; */
}

.icon {
  height: 120px;
  display: block;
  padding: 10px;
  color: #FFFFFF;

}

.iconLeft {
  width: 120px;
  float: left;
  display: inline-block;
}

.iconLeftItem {
  width: 120px;
  height: 120px;
  text-align: center;
}

.iconTitle {
  font-size: 13px;
  line-height: 20px;
}

.iconDesc {
  font-size: 11px;
}

.iconRight {
  padding-left: 10px;
  width: calc(100vw - 150px);
  display: inline-block;
}

.iconRightItem {
  height: 55px;
  display: block;
}

.iconBottomItem {
  margin-top: 10px;
}
.iconRightItemLeft {
  width: 80px;
  height: 55px;

  display: inline-block;
}
.iconRightItemRight {
  display: inline-block;
  
}
.category {
  padding: 10px 10px 0 10px;
  font-size: 15px;
}

.categoryTitle {
  display: inline-block;
}

.verticalLine {
  width: 3px;
  height: 16px;
  background: rgba(0, 196, 168, 1);
  display: inline-block;
}

.categoryText {
  display: inline-block;
  padding-left: 5px;
  color: #333333;
}

.article {
  height: 150px;
  display: inline-block;
  padding-top: 10px;
  padding-bottom: 10px;
  color: #FFFFFF;
  width: 100%;
}

.articleLeft {
  height: 135px;
  width: 30%;
  display: inline-block;
  vertical-align: top;
}

.articleLeftItem {
  height: 135px;
  background: rgba(0, 0, 0, 1);
  opacity: 0.7;
  border-radius: 5px;
  padding: 5px;
  margin-left: 10px;
}

.articleMiddle {
  width: 35%;
  display: inline-block;
  height: 150px;
  vertical-align: top;
}

.articleRight {
  width: 35%;
  display: inline-block;
  height: 150px;
  vertical-align: top;
}

.articleItem {
  background: rgba(0, 0, 0, 1);
  opacity: 0.7;
  border-radius: 5px;
  height: 60px;
  margin-left: 5px;
  padding: 5px;
}

.articleBottomItem {
  margin-top: 5px;
}

.articleRightItem {
  margin-right: 10px;
}


.articleTitle {
  /* padding-top: 10px; */
  font-size: 13px;
  padding-bottom: 10px;
  height: 48px;
  line-height: 18px;
    -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}

.articleDesc {
  /* height: 50px; */
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 12px;
}

.articleLeftDesc {
  height: 60px;
  line-height: 20px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  font-size: 12px;
}

.tip {
  position: absolute;
  top: 0px;
  background: rgba(0, 0, 0, 1);
  opacity: 0.5;
  height: 100vh;
  width: 100vw;
  overflow: hidden;
}

.tipInfo {
  position: absolute;
  top: 280px;
  right: 120px;
}

.tipArrow {
  position: absolute;
  top: 310px;
  right: 50px;
}

.tipKnow {
  position: absolute;
  bottom: 20px;
  left: 50%;
  transform: translate(-50%, -50%);
}

.tipInfoImage {
  width: 180px;
}

.tipArrowImage {
  width: 60px;
}

.tipKnowImage {
  width: 180px;
}

.comment {
  margin: 10px;
  background: rgba(255, 255, 255, 1);
  box-shadow: 1px 2px 9px 0px rgba(0, 0, 0, 0.3);
  border-radius: 4px;
  height: 80px;
}

.commentItem {
  padding: 10px;
}

.commentHead {
  width: 60px;
  height: 85px;
  float: left;
}

.commentContent {
  height: 85px;
  margin-left: 70px;
}

.commentTitle {
  height: 40px;
  line-height: 20px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  font-size: 13px;
  color: #666666;
}

.commentBottom {
  font-size: 12px;
  padding-top: 7px;
  display: inline-block;
}

.commentUsername {
  color: #999999;
  display: inline-block;
}

.commentCategory {
  color: #00C4A8;
  padding-left: 10px;
  display: inline-block;
}

.commentDate {
  color: #999999;
  right: 10px;
  position: absolute;
  display: inline-block;
}